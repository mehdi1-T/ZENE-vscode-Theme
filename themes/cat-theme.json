  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a6a8a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#ff006e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#FFB6C1"
      }
    },
    {
      "name": "Classes, Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#b300ff"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#b300ff"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#ffaa00"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e0e0e8"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#00ddff"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff0088"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#ff006e"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00f5ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import/Export Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#ff006e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#ff006e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ff006e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#b300ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00f5ff"
      }
    },
    {
      "name": "Markup Links",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#00f5ff",
        "fontStyle": "underline"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#00f5ff"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#b300ff"
      }
    },
    {
      "name": "CSS IDs",
      "scope": ["entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#ff006e"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#00f5ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#9090a0"
      }
    },
    {
      "name": "Invalid/Deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#ff0044",
        "fontStyle": "bold underline"
      }
    }
  ]
}