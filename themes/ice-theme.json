  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e7681",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#00cc66"
      }
    },
    {
      "scope": ["string.template", "string.interpolated"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "scope": ["punctuation.definition.template-expression", "punctuation.section.embedded"],
      "settings": {
        "foreground": "#ff6bff"
      }
    },
    {
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null"],
      "settings": {
        "foreground": "#ff9d3d"
      }
    },
    {
      "scope": ["constant.language", "support.constant"],
      "settings": {
        "foreground": "#ff6bff"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff6bff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#ff6bff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#00d4ff"
      }
    },
    {
      "scope": ["storage", "storage.type.function", "storage.type.class"],
      "settings": {
        "foreground": "#ff6bff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#e3e8ef"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super"],
      "settings": {
        "foreground": "#ff9d3d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#00d4ff"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#00d4ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff6b85"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00d4ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#00d4ff"
      }
    },
    {
      "scope": ["meta.object-literal.key", "meta.property-name"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["punctuation.decorator", "meta.decorator"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff6b85",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#00d4ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ff6bff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#00d4ff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#8b949e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#00d4ff"
      }
    },
    {
      "scope": ["source.json string.quoted.double.json"],
      "settings": {
        "foreground": "#00ff88"
      }
    }
  ]
}