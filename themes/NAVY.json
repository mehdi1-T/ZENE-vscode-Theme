  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#4a6f84",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#9ce3d0"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#7fc9b5"
      }
    },
    {
      "scope": ["string.template", "string.interpolated"],
      "settings": {
        "foreground": "#9ce3d0"
      }
    },
    {
      "scope": ["punctuation.definition.template-expression", "punctuation.section.embedded"],
      "settings": {
        "foreground": "#A78BFA"
      }
    },
    {
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#ffb74d"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null"],
      "settings": {
        "foreground": "#ffb74d"
      }
    },
    {
      "scope": ["constant.language", "support.constant"],
      "settings": {
        "foreground": "#9dabff"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#7fc7ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#7fc7ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#7fdbff"
      }
    },
    {
      "scope": ["storage", "storage.type.function", "storage.type.class"],
      "settings": {
        "foreground": "#A78BFA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#bcd5e3"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super"],
      "settings": {
        "foreground": "#ffb74d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#bcd5e3"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#7fdbff"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#ffb74d"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#ffb74d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#5fb3f6"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#9ce3d0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#6094ad"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#7fdbff"
      }
    },
    {
      "scope": ["meta.object-literal.key", "meta.property-name"],
      "settings": {
        "foreground": "#bcd5e3"
      }
    },
    {
      "scope": ["punctuation.decorator", "meta.decorator"],
      "settings": {
        "foreground": "#ffb74d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ef5350",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#7fdbff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffb74d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#A78BFA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#9ce3d0"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7fdbff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#6094ad",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#7fdbff"
      }
    },
    {
      "scope": ["source.json string.quoted.double.json"],
      "settings": {
        "foreground": "#9ce3d0"
      }
    }
  ]
}