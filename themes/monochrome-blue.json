    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["storage", "storage.type.function", "storage.type.class"],
      "settings": {
        "foreground": "#219fd5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super"],
      "settings": {
        "foreground": "#219fd5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#219fd5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#219fd5"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#219fd5"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#219fd5"
      }
    },
    {
      "scope": ["meta.object-literal.key", "meta.property-name"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["punctuation.decorator", "meta.decorator"],
      "settings": {
        "foreground": "#219fd5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff0000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#219fd5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#219fd5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#219fd5",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#219fd5"
      }
    },
    {
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["source.json string.quoted.double.json"],
      "settings": {
        "foreground": "#219fd5"
      }
    }
  ]
}
