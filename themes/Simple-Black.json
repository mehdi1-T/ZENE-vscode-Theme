  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e7681",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#6dffb3"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#52e096"
      }
    },
    {
      "scope": ["string.template", "string.interpolated"],
      "settings": {
        "foreground": "#6dffb3"
      }
    },
    {
      "scope": ["punctuation.definition.template-expression", "punctuation.section.embedded"],
      "settings": {
        "foreground": "#6dd0ff"
      }
    },
    {
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#e3e8ef"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null"],
      "settings": {
        "foreground": "#ffd93d"
      }
    },
    {
      "scope": ["constant.language", "support.constant"],
      "settings": {
        "foreground": "#ff9d6f"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#89ddff"
      }
    },
    {
      "scope": ["storage", "storage.type.function", "storage.type.class"],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#e3e8ef"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super"],
      "settings": {
        "foreground": "#ff9d6f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#e3e8ef"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#6dd0ff"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#ffd93d"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#ffd93d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffd93d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#6dd0ff"
      }
    },
    {
      "scope": ["meta.object-literal.key", "meta.property-name"],
      "settings": {
        "foreground": "#6dd0ff"
      }
    },
    {
      "scope": ["punctuation.decorator", "meta.decorator"],
      "settings": {
        "foreground": "#ffd93d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff6b6b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#6dd0ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffd93d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#6dffb3"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#6dd0ff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#8b949e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#6dd0ff"
      }
    },
    {
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#ffd93d"
      }
    },
    {
      "scope": ["source.json string.quoted.double.json"],
      "settings": {
        "foreground": "#6dffb3"
      }
    }
  ]
}