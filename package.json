      {
        "label": "Monochromator Light Aquamarine",
        "uiTheme": "vs",
        "path": "./themes/light/aquamarine.json"
      },
      {
        "label": "Monochromator Light Emerald",
        "uiTheme": "vs",
        "path": "./themes/light/emerald.json"
      },
      {
        "label": "Monochromator Light Plain",
        "uiTheme": "vs",
        "path": "./themes/light/plain.json"
      },
      {
        "label": "Monochromator Light Ruby",
        "uiTheme": "vs",
        "path": "./themes/light/ruby.json"
      },
      {
        "label": "Monochromator Light Sulfur",
        "uiTheme": "vs",
        "path": "./themes/light/sulfur.json"
      }
    ]
  },
  "vsce": {
    "baseContentUrl": "https://codeberg.org/beem/monochromator/raw/branch/master"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.19.22",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.6.2",
    "typescript": "^5.9.3"
  }
}