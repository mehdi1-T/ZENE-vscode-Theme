{
  "name": "macos-sonoma-theme",
  "displayName": "macOS Sonoma Theme",
  "description": "A macOS-inspired VS Code theme with clean, professional design",
  "version": "3.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "macos",
    "sonoma",
    "apple",
    "clean",
    "professional",
    "dark theme",
    "coding",
    "programming",
    "modern",
    "minimal"
  ],
  "publisher": "mehditalalha",
  "license": "MIT",
  "icon": "static/logo.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/mehdi1-T/ZENE-vscode-Theme.git"
  },
  "bugs": {
    "url": "https://github.com/mehdi1-T/ZENE-vscode-Theme/issues",
    "email": "mehditalalha29@gmail.com"
  },
  "contributes": {
    "themes": [
      {
        "label": "Zene Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/zene-vs-code-theme.json"
      }
    ],
    "configuration": {
      "title": "macOS Sonoma",
      "properties": {
        "sonoma.blurEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable macOS Sonoma blur effects for reduced eye strain"
        }
      }
    }
  },
  "vsce": {
    "baseContentUrl": "https://codeberg.org/beem/monochromator/raw/branch/master"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.19.22",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.6.2",
    "typescript": "^5.9.3"
  }
}