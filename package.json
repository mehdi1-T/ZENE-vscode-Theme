      },
      {
        "label": "Monochromator Dark Amber",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/amber.json"
      },
      {
        "label": "Monochromator Dark Amethyst",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/amethyst.json"
      },
      {
        "label": "Monochromator Dark Aquamarine",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/aquamarine.json"
      },
      {
        "label": "Monochromator Dark Emerald",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/emerald.json"
      },
      {
        "label": "Monochromator Dark Plain",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/plain.json"
      },
      {
        "label": "Monochromator Dark Ruby",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/ruby.json"
      },
      {
        "label": "Monochromator Dark Sulfur",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/sulfur.json"
      },
      {
        "label": "Monochromator Light",
        "uiTheme": "vs",
        "path": "./themes/light/default.json"
      },
      {
