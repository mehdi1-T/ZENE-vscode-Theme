{
  "name": "zene-theme",
  "displayName": "Zene Theme",
  "description": "A friendly and clean VS Code theme.",
  "version": "3.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "zene",
    "theme",
    "Zene Theme",
    "coder coder dark",
    "monochromator",
    "clean",
    "professional",
    "dark theme",
    "coding",
    "programming",
    "modern",
    "minimal"
  ],
  "publisher": "mehditalalha",
  "license": "MIT",
  "icon": "static/logo.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/mehdi1-T/ZENE-vscode-Theme.git"
  },
  "bugs": {
    "url": "https://github.com/mehdi1-T/ZENE-vscode-Theme/issues",
    "email": "mehditalalha29@gmail.com"
  },
  "contributes": {
    "themes": [
      {
        "label": "Zene Dark Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/zene-dark-vs-code-theme.json"
      },
      {
        "label": "Zene Light Theme",
        "uiTheme": "hc-light",
        "path": "./themes/zene-light-vs-code-theme.json"
      }
    ],
    "commands": [
      {
        "command": "zene.toggleSidebarBlur",
        "title": "Toggle Sidebar Blur",
        "category": "Zene Theme"
      }
    ],
    "configuration": {
      "title": "Zene Theme",
      "properties": {
        "Zene.sidebarBlurEnabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable Zene Theme macOS-style blur effect for sidebar and file explorer"
        }
      }
    }
  },
  "vsce": {
    "baseContentUrl": "https://github.com/mehdi1-T/ZENE-vscode-Theme"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:zene.toggleSidebarBlur"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.19.22",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.6.2",
    "typescript": "^5.9.3"
  }
}